#include <iostream>
#include <math.h>
#include <stdlib.h>

using namespace std;

class Stack
{
    struct Element
    {
        Element* prev;
        float value;
    };

    Element* t;

public:
    Stack() {
        t -> prev = NULL;
    }    // inicjalizuje pusty stos
    ~Stack() {}   // kasuje wszystkie elementy

    void pop() {

        if(t == NULL) {return; }
        t = t -> prev;

    }  // kasuje gorny element stosu


    float top() {

    return t -> value;

    }  // zwraca wartosc na gorze stosu

    bool empty() {

        if(t -> prev == NULL) {return 1;}
                            else {return 0;}

    }// true = stos jest pusty
    void push(float a) {

    t -> value = a;

    } // umieszcza wartosc na wierzcholku stosu
};

int main()
{
    Stack s;

    while (!cin.eof()) {
        string token;
        cin >> token;

        if (token == "+") {
            float l, r;

            r = s.top(); s.pop();
            l = s.top(); s.pop();

            s.push(l + r);
        } else if (token == "*") {
            float l, r;

            r = s.top(); s.pop();
            l = s.top(); s.pop();

            s.push(l * r);
        } else if (token == "=") {
            cout << s.top() << endl;
        } else {
            s.push(atof(token.c_str()));
        }
    }
}
